{
  "name": "covid-react-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://evanherman.github.io/COVID-app/",
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "ajv": "^6.9.1",
    "flag-icons": "^5.0.2",
    "material-table": "^1.69.3",
    "react-scripts": "5.0.0",
    "web-vitals": "^2.1.2"
  },
  "scripts": {
    "prepare": "husky install .dev/husky",
    "start": "npm run copy-flags && react-scripts start",
    "build": "npm run copy-flags && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "copy-flags": "cp -r ./node_modules/flag-icons/css ./src/components/countries-data-table/ && cp -r ./node_modules/flag-icons/flags ./src/components/countries-data-table/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-google-charts": "^3.0.15",
    "react-responsive-modal": "^6.2.0",
    "tailwindcss": "^3.0.7",
    "typescript": "^4.5.4"
  }
}
